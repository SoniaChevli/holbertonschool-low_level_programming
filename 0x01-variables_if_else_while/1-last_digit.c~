#include <stdlib.h>
#include <time.h>
/* more headers goes there */

/* betty style doc for function main goes there */
int main(void)
{
  int n;
  int x;

  srand(time(0));
  n = rand() - RAND_MAX / 2;
  
  x = n % 10;
  printf("Last digit of %d is %d", n, x); 

 /* your code goes there */
  return (0);
}
